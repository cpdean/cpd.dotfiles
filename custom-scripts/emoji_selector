#!/usr/bin/env bash
function main {
    selection=$(echo "$emoji_list" | rofi -dmenu -p emoji -l 5)
    if [ $? = 0 ]; then
        moji=$(echo "$selection" | awk '{print $1}' | tr -d '\n')
        echo $moji | xclip -i -selection clipboard
        notify-send "$moji copied to clipboard"
    fi
}

